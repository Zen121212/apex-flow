# ApexFlow React App Environment Variables
# Copy this file to .env.local and update the values as needed

# API URLs - Update these to match your backend services
VITE_API_BASE_URL=http://localhost:3000
VITE_AGENT_ORCHESTRATOR_URL=http://localhost:3002

# App Configuration
VITE_APP_NAME=ApexFlow
VITE_APP_VERSION=1.0.0

# Feature Flags - Enable/disable features
VITE_ENABLE_AI_PROCESSING=true
VITE_ENABLE_WORKFLOWS=true
VITE_ENABLE_VECTOR_SEARCH=true

# Development settings
VITE_LOG_LEVEL=info
VITE_ENABLE_DEBUG=true
